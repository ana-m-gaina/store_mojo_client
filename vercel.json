{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "/media/:path*"
    },
    {
      "source": "/<^/(.*)",
      "destination": "/<^/"
    },
    {
      "source": "/cart",
      "destination": "/"
    },
    {
      "source": "/checkout-success",
      "destination": "/"
    },
    {
      "source": "/account/:id",
      "destination": "/"
    },
    {
      "source": "/orders/:id",
      "destination": "/"
    },
    {
      "source": "/products/find/:id",
      "destination": "/"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/index.js"
    },
    {
      "src": "/<^/(.*)",
      "dest": "/<^.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/index.js"
    },
    {
      "src": "/cart",
      "dest": "/index.html"
    },
    {
      "src": "/checkout-success",
      "dest": "/index.html"
    },
    {
      "src": "/account/:id",
      "dest": "/index.html"
    },
    {
      "src": "/orders/:id",
      "dest": "/index.html"
    },
    {
      "src": "/products/find/:id",
      "dest": "/index.html"
    },
    {
      "src": "/:path*",
      "dest": "/index.html"
    }
  ]
}
